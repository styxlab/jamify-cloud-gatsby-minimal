        "cross-env": "7.0.2",
        "typescript": "^3.9.5"
    },
    "peerDependencies": {
        "gatsby": "^2.24.27"
    },
    "dependencies": {
        "@babel/runtime": "7.11.0",
        "crypto": "^1.0.1",
